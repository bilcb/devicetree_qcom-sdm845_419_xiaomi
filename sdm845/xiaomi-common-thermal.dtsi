&pm8998_vadc {
	vph_pwr@83 {
		label = "vph_pwr";
		reg = <ADC_VPH_PWR>;
		qcom,pre-scaling = <1 3>;
	};

	vcoin@85 {
		label = "vcoin";
		reg = <ADC_VCOIN>;
		qcom,pre-scaling = <1 3>;
	};

	xo_therm@4c {
		label = "xo_therm";
		reg = <ADC_XO_THERM_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	cam_therm0@4d {
		label = "cam_therm0";
		reg = <ADC_AMUX_THM1_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	cam_therm1@4e {
		label = "cam_therm1";
		reg = <ADC_AMUX_THM2_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	pa_therm0@4f {
		label = "pa_therm0";
		reg = <ADC_AMUX_THM3_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	pa_therm1@50 {
		label = "pa_therm1";
		reg = <ADC_AMUX_THM4_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};

	quiet_therm@51 {
		label = "quiet_therm";
		reg = <ADC_AMUX_THM5_PU2>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

